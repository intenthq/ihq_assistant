      "deletions": 1,
      "changes": 36,
      "blob_url": "https://github.com/intenthq/ihq_assistant/blob/77189e6a5c0ed620a966d4b608f22a7bd6317578/rag_util_functions.py",
      "raw_url": "https://github.com/intenthq/ihq_assistant/raw/77189e6a5c0ed620a966d4b608f22a7bd6317578/rag_util_functions.py",
